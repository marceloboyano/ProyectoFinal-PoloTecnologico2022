@model IEnumerable<ProyectoFinal.Data.HistorialCartones>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Carton1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Carton2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Carton3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Carton4)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Carton1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Carton2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Carton3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Carton4)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FechaHora">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FechaHora">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FechaHora">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
